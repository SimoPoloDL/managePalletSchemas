<mvc:View
   xmlns:core="sap.ui.core"
   xmlns:mvc="sap.ui.core.mvc"
   xmlns:c="sap.ui.commons"
   xmlns="sap.m"
   xmlns:l="sap.ui.layout"
   xmlns:html="http://www.w3.org/1999/xhtml"
   xmlns:t="sap.ui.table"
   xmlns:f="sap.ui.layout.form"
   xmlns:comp="managepalletschemas.component"
   xmlns:u="sap.ui.unified"
   controllerName="managepalletschemas.controller.Main">
   <Page title="{i18n>managepalletschemas.view.title}" class="myPage" showHeader="true" enableScrolling="false">
      <!-- <headerContent> <Label text="{i18n>managepalletschemas.view.user.label}" class="sapUiTinyMarginEnd"/> <Label text="{info>/user/name}" class="sapUiMediumMarginEnd whiteText"/> <Button press="logOff" icon="sap-icon://log"/> <Image id="DLLogo"
      class="DLLogo" src="images/Delonghi.png" densityAware="false" height="75%"/> </headerContent> -->
      <customHeader>
         <Bar>
            <contentLeft>
               <Button icon="sap-icon://nav-back" press="toBackPage"/>
            </contentLeft>
            <contentMiddle>
               <Label text="{i18n>managepalletschemas.view.title}" class="sapMTitle sapMTitleStyleAuto sapMTitleNoWrap sapUiTinyMarginEnd"/>
            </contentMiddle>
            <contentRight>
               <Label text="{i18n>managepalletschemas.view.user.label}" class="sapUiTinyMarginEnd"/>
               <Label text="{info>/user/name}" class="sapUiMediumMarginEnd"/>
               <Button press="logOff" icon="sap-icon://log"/>
